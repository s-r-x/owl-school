{
  "name": "void",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all -s webpack-prod gulp-prod",
    "prebuild": "npm run-script clean",
    "postbuild": "npm run-script print-bundle-size",
    "start": "npm-run-all -p webpack-dev gulp-dev",
    "prestart": "npm run-script clean",
    "print-bundle-size": "node internals/print-bundle-size.js",
    "serve": "http-server ./dist -p 8080 --cors -o",
    "images": "gulp --gulpfile internals/gulp-config/gulpfile.js images_prod",
    "clean": "cleandir ./dist",
    "webpack-dev": "NODE_ENV=development webpack --mode=development --config internals/webpack-config/webpack.development.js",
    "webpack-prod": "NODE_ENV=production webpack --mode=production --config internals/webpack-config/webpack.production.js",
    "gulp-dev": "NODE_ENV=development gulp --gulpfile internals/gulp-config/gulpfile.js",
    "gulp-prod": "NODE_ENV=production gulp --gulpfile internals/gulp-config/gulpfile.js"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 3 version",
    "> 1%",
    "not ie < 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "clean-dir": "^1.0.3",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-ejs": "^3.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-watch": "^5.0.1",
    "gzip-size": "^5.0.0",
    "http-server": "^0.11.1",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettysize": "^1.1.0",
    "regenerator-runtime": "^0.12.1",
    "svgo": "1.0.5",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "gsap": "^2.0.2",
    "jquery": "^3.3.1",
    "jump.js": "^1.0.2",
    "normalize.css": "^8.0.0",
    "scrollreveal": "^4.0.5",
    "slick-carousel": "^1.8.1"
  }
}
